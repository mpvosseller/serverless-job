{
  "name": "serverless-job",
  "version": "0.0.3",
  "author": "Michael Vosseller",
  "license": "MIT",
  "description": "A framework for declaring and running jobs in Node.js on AWS Lambda with Amazon SQS.",
  "keywords": [
    "active",
    "job",
    "activejob",
    "active job",
    "jobs",
    "aws",
    "lambda",
    "amazon",
    "sqs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpvosseller/serverless-job.git"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/lib/**/!(*.test).d.ts",
    "dist/lib/**/!(*.test).js"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix 'src/**'",
    "lint:nofix": "eslint 'src/**'",
    "build": "tsc",
    "clean": "rm -rf dist/",
    "watch": "tsc -w",
    "test": "jest",
    "prepublishOnly": "yarn clean && yarn lint:nofix && yarn test && yarn build"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "aws-sdk": "^2.906.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "aws-sdk": "^2.906.0"
  },
  "dependencies": {
    "glob": "^7.1.7"
  }
}
